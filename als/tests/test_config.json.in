{
  "server": {
    "maxCachedDocuments": 10,
    "completionTimeout": 100,
    "diagnosticsDelay": 50,
    "maxWorkerThreads": 2,
    "logLevel": "debug"
  },
  "analysis": {
    "enableSemanticAnalysis": true,
    "enableTypeInference": true
  },
  "completion": {
    "enableSnippets": false,
    "maxSuggestions": 10
  },
  "diagnostics": {
    "enableLinting": true
  },
  "test": {
    "timeout": 5000,
    "dataDirectory": "@CMAKE_CURRENT_BINARY_DIR@/test_data"
  }
}
